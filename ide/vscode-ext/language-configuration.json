{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"],
    ["{", "}"],
    ["if", "end"],
    ["fun", "end"],
    ["type", "end"],
    ["while", "end"],
    ["for", "end"],
    ["impl", "end"],
    ["try", "end"],
    ["match", "end"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" },
    { "open": "\"", "close": "\"" }
  ],
  "surroundingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" },
    { "open": "\"", "close": "\"" }
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(if|fun|type|impl|while|for|match|try)\\b",
      "end": "^\\s*end\\b"
    }
  },
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(if|elif|else|fun|type|impl|while|for|match|try|catch|ensure)\\b.*$|^\\s*in\\s+.*$",
    "decreaseIndentPattern": "^\\s*end\\b.*$"
  }
}
